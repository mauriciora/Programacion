// Valor Absoluto.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main ()
{
	int numero;
	cout<<"Ingrese Numero: ";
	cin>>numero;
	if (numero >=0)
		cout<<"El Valor Absoluto es:"<<numero;
	else 
		cout <<"El Valor Absoluto es:"<<-1*numero;
	getch();
	
}
